name: "Dog2D_Critic"

input: "data"
input_dim:1
input_dim:1
input_dim:1
input_dim:283

layer {
	name: "slice0"
	type: "Slice"
	bottom: "data"
	top: "data_terrain"
	top: "data_char"
	slice_param {
		axis: 3
		slice_point: 200
	}
}

layer {
  name: "terr_conv0"
  type: "Convolution"
  bottom: "data_terrain"
  top: "terr_conv0"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 16
    kernel_h: 1
	kernel_w: 8
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "terr_relu0"
  type: "ReLU"
  bottom: "terr_conv0"
  top: "terr_relu0"
}

layer {
  name: "terr_conv1"
  type: "Convolution"
  bottom: "terr_relu0"
  top: "terr_conv1"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 32
    kernel_h: 1
	kernel_w: 4
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "terr_relu1"
  type: "ReLU"
  bottom: "terr_conv1"
  top: "terr_relu1"
}

layer {
  name: "terr_conv2"
  type: "Convolution"
  bottom: "terr_relu1"
  top: "terr_conv2"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 32
    kernel_h: 1
	kernel_w: 4
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "terr_relu2"
  type: "ReLU"
  bottom: "terr_conv2"
  top: "terr_relu2"
}

layer {
  name: "terr_ip0"
  type: "InnerProduct"
  bottom: "terr_relu2"
  top: "terr_ip0"
  param {
    lr_mult: 1
	decay_mult: 1
  }
  param {
    lr_mult: 2
	decay_mult: 0
  }
  inner_product_param {
    num_output: 64
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "terr_relu3"
  type: "ReLU"
  bottom: "terr_ip0"
  top: "terr_relu3"
}

layer {
	name: "char_flatten0"
	type: "Flatten"
	bottom: "data_char"
	top: "char_flatten0"
}

layer {
	name: "concat0"
	type: "Concat"
	bottom: "terr_relu3"
	bottom: "char_flatten0"
	top: "concat0"
	concat_param {
		axis: 1
	}
}

layer {
  name: "ip1"
  type: "InnerProduct"
  bottom: "concat0"
  top: "ip1"
  param {
    lr_mult: 1
	decay_mult: 1
  }
  param {
    lr_mult: 2
	decay_mult: 0
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "relu3"
  type: "ReLU"
  bottom: "ip1"
  top: "relu3"
}

layer {
  name: "ip2"
  type: "InnerProduct"
  bottom: "relu3"
  top: "ip2"
  param {
    lr_mult: 1
	decay_mult: 1
  }
  param {
    lr_mult: 2
	decay_mult: 0
  }
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "relu4"
  type: "ReLU"
  bottom: "ip2"
  top: "relu4"
}

layer {
  name: "output"
  type: "InnerProduct"
  bottom: "relu4"
  top: "output"
  param {
    lr_mult: 1
	decay_mult: 1
  }
  param {
    lr_mult: 2
	decay_mult: 0
  }
  inner_product_param {
    num_output: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}